FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            symmetry;
    }
    down
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            symmetry;
    }
    solid_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}


