FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      Cp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 -1 0 0 0 ];

internalField   uniform 490;

boundaryField
{
    walls
    {
        type            symmetry;
    }
    down
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            symmetry;
    }
    solid_to_fluid
    {
        type            zeroGradient;
        value           uniform 0;
    }
}


