FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    up
    {
        type            fixedValue;
        value           uniform 0;
    }
    walls
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            symmetry;
    }
    fluid_to_solid
    {
        type            zeroGradient;
        value           uniform 0;
        gradient        uniform 0;
    }
}


