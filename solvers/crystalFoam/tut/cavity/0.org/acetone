FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      acetone;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.12;

boundaryField
{
    movingWall //acetone-HMX
    {
    	type            fixedValue;
    	value	    	uniform 0.12; //mass fraction due to table 1
    }
    "(fixedWalls|defaultFaces)"
    {
    	type            zeroGradient;
    }
}


