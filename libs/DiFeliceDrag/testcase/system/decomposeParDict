FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 8;

method          simple;

simpleCoeffs
{
    n               (1 2 4);
}
constraints
{
    preservePatches1
    {
        type            preservePatches;
        patches         (top bottom);
    }
}

